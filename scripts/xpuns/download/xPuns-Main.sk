		set {xPuns::banned::%uuid of arg-1%} to {_id}
command /ipban <text> [<text>]:
	permission: xpuns.ipban
	permission message: &c&lxPuns&8 » {@noperm}
	usage: &c&lxPuns&8 » &cUsage: /ipban (player | IP) (reason) (-n) (-s)
	aliases: xpuns:ipban, ip-ban, xpuns:ip-ban, banip, xpuns:banip, ban-ip, xpuns:ban-ip
	trigger:
		set {_id} to "ipban-%floor(unix timestamp of now)%"
		set {_r} to "No reason given"
		set {_n} to "N/A"
		set {_d} to "Permanent"
		set {_sl} to false
		if arg-2 is set:
			set {_r} to xPunsGetReason(arg-2)
			set {_n} to xPunsGetNote(arg-2)
			set {_sl} to xPunsCheckForSilent(arg-2)
		set {_ip::*} to arg-1 split at "."
		set {_fromip} to true
		if size of {_ip::*} is 4:
			loop {_ip::*}:
				set {_n} to loop-value parsed as integer
				if {_n} is not an integer:
					set {_fromip} to false
					stop loop
			if {_fromip} is false:
				message "&c&lxPuns&8 » &cInvalid IP"
				stop
			if {_fromip} is true:
				set {_u::*} to xPunsFetchAccountsFromIP(arg-1)
				if size of {_u::*} < 1:
					message "&c&lxPuns&8 » &cNo accounts are linked to this IP"
					stop
				loop {_u::*}:
					set {_p} to loop-value parsed as offline player
					if xPunsCheckPerm(sender, {_p}) is false:
						message "&c&lxPuns&8 » &cYou cannot punish %{_p}%"
						stop
				loop {_u::*}:
					set {_p} to loop-value parsed as offline player
					set {xPuns::banned::%loop-value%} to {_id}
					xPunsAddPunishment(sender, {_p}, "IP-Ban", {_r}, {_n}, "Permanent", {_sl}, {_id})
					if {_n} is not "N/A":
						kick {_p} because "&7%nl%&c&lxPuns&8 » &cYou have been IP-banned!%nl%&8 » &7Reason&8: &6%{_r}%%nl%&8 » &7Duration&8: &c%xPunsGetTimespan({_d})%%nl%&8 » &7Issued by&8: &b%sender%%nl%&c%{_n}%"
					else:
						kick {_p} because "&7%nl%&c&lxPuns&8 » &cYou have been IP-banned!%nl%&8 » &7Reason&8: &6%{_r}%%nl%&8 » &7Duration&8: &c%xPunsGetTimespan({_d})%%nl%&8 » &7Issued by&8: &b%sender%"
				xPunsMessageIP("ipbanIP", arg-1, "%sender%", {_r}, "PERMANENT", {_sl})
				stop
		set {_p} to arg-1 parsed as offline player
		if xPunsCheckPerm(sender, {_p}) is false:
			message "&c&lxPuns&8 » &cYou cannot punish %{_p}%"
			stop
		set {_u::*} to xPunsFetchAccountsFromPlayer({_p})
		if size of {_u::*} < 1:
			message "&c&lxPuns&8 » &cNo IP's are linked to this account"
			stop
		xPunsMessage("ipban", {_p}, "%sender%", {_r}, "PERMANENT", {_n}, {_sl})
		loop {_u::*}:
			set {_p} to loop-value parsed as offline player
			xPunsAddPunishment(sender, {_p}, "IP-Ban", {_r}, {_n}, "Permanent", {_sl}, {_id})
			set {xPuns::banned::%loop-value%} to {_id}
			set {_p} to loop-value parsed as offline player
			if {_n} is not "N/A":
				kick {_p} because "&7%nl%&c&lxPuns&8 » &cYou have been IP-banned!%nl%&8 » &7Reason&8: &6%{_r}%%nl%&8 » &7Duration&8: &c%xPunsGetTimespan({_d})%%nl%&8 » &7Issued by&8: &b%sender%%nl%&c%{_n}%"
			else:
				kick {_p} because "&7%nl%&c&lxPuns&8 » &cYou have been IP-banned!%nl%&8 » &7Reason&8: &6%{_r}%%nl%&8 » &7Duration&8: &c%xPunsGetTimespan({_d})%%nl%&8 » &7Issued by&8: &b%sender%"
command /unmute <offline player> [<text>]:
	permission: xpuns.unmute
	permission message: &c&lxPuns&8 » {@noperm}
	usage: &c&lxPuns&8 » &cUsage: /unmute (player) (-s)
	aliases: xpuns:unmute
	trigger:
		if xPunsCheckMute(arg-1) is false:
			message "&c&lxPuns&8 » &c%arg-1% &cis not muted"
			stop
		if arg-2 contains "-s":
			xPunsMessage("unmute", arg-1, "%sender%", "N/A", "PERMANENT", "N/A", true)
		else:
			xPunsMessage("unmute", arg-1, "%sender%", "N/A", "PERMANENT", "N/A", false)
		clear {xPuns::muted::%uuid of arg-1%}
command /unban <offline player> [<text>]:
	permission: xpuns.unban
	permission message: &c&lxPuns&8 » {@noperm}
	usage: &c&lxPuns&8 » &cUsage: /unban (player) (-s)
	aliases: xpuns:unban
	trigger:
		if xPunsCheckBan(arg-1) is false:
			message "&c&lxPuns&8 » &c%arg-1% &cis not banned"
			stop
		if arg-2 contains "-s":
			xPunsMessage("unban", arg-1, "%sender%", "N/A", "PERMANENT", "N/A", true)
		else:
			xPunsMessage("unban", arg-1, "%sender%", "N/A", "PERMANENT", "N/A", false)
		clear {xPuns::banned::%uuid of arg-1%}
command /unipban <text> [<text>]:
	permission: xpuns.unmute
	permission message: &c&lxPuns&8 » {@noperm}
	usage: &c&lxPuns&8 » &cUsage: /unbanip (IP) (-s)
	aliases: xpuns:unipban, unbanip, xpuns:unbanip
	trigger:
		set {_ip::*} to arg-1 split at "."
		if size of {_ip::*} is not 4:
			message "&c&lxPuns&8 » &cInvalid IP"
			stop
		loop {_ip::*}:
			set {_n} to loop-value parsed as integer
			if {_n} is not an integer:
				message "&c&lxPuns&8 » &cInvalid IP"
				stop
		if xPunsCheckIPbanFromIP(arg-1) is false:
			message "&c&lxPuns&8 » &cIP %arg-1% &cis not banned"
			stop
		if arg-2 contains "-s":
			xPunsMessageIP("unipban", arg-1, "%sender%", "N/A", "PERMANENT", true)
		else:
			xPunsMessageIP("unipban", arg-1, "%sender%", "N/A", "PERMANENT", false)
		set {_u::*} to xPunsFetchAccountsFromIP(arg-1)
		loop {_u::*}:
			delete {xPuns::banned::%loop-value%}
command /alts <offline player>:
	permission: xpuns.alts
	permission message: &c&lxPuns&8 » {@noperm}
	usage: &c&lxPuns&8 » &cUsage: /alts (player)
	aliases: xpuns:alts
	trigger:
		message "&c&lxPuns&8 » &cAlts &8(&b%arg-1%&8)"
		set {_p::*} to all players
		set {_u::*} to xPunsFetchAccountsFromPlayer(arg-1)
		if size of {_u::*} > 1:
			loop {_u::*}:
				set {_p} to loop-value parsed as offline player
				if xPunsCheckBan({_p}) is true:
					message "&8 » &c%{_p}%"
				else if xPunsCheckMute({_p}) is true:
					message "&8 » &6%{_p}%"
				else if {_p::*} contains {_p}:
					message "&8 » &a%{_p}%"
				else:
					message "&8 » &7%{_p}%"
			stop
		if xPunsCheckBan(arg-1) is true:
			message "&8 » &c%arg-1%"
		else if xPunsCheckMute(arg-1) is true:
			message "&8 » &6%arg-1%"
		else if {_p::*} contains arg-1:
			message "&8 » &a%arg-1%"
		else:
			message "&8 » &7%arg-1%"
command /ips <offline player>:
	permission: xpuns.ips
	permission message: &c&lxPuns&8 » {@noperm}
	usage: &c&lxPuns&8 » &cUsage: /ips (player)
	aliases: xpuns:ips
	trigger:
		set {_ips::*} to xPunsFetchIPs(arg-1)
		if size of {_ips::*} >= 1:
			message "&c&lxPuns&8 » &cIP's &8(&b%arg-1%&8)"
			loop {_ips::*}:
				execute console command "tellraw %player% {""text"":""&8 » &b%loop-value%"",""clickEvent"":{""action"":""suggest_command"",""value"":""%loop-value%""},""hoverEvent"":{""action"":""show_text"",""contents"":[""&aClick to copy""]}}"
			stop
		message "&cxPuns&8 » &c%arg-1% &chas no known IP's"
command /history <offline player> [<text>]:
	permission: xpuns.history
	permission message: &c&lxPuns&8 » {@noperm}
	usage: &c&lxPuns&8 » &cUsage: /history (player) (clear)
	aliases: xpuns:history, puns, xpuns:puns
	trigger:
		if arg-2 is "clear":
			if player has permission "xpuns.history.clear":
				clear {xPuns::received::%uuid of arg-1%::*}
				message "&c&lxPuns&8 » &a%arg-1%&a's punishment history has been cleared"
				stop
			message "&c&lxPuns&8 » &cYou do not have permission to clear history"
		if size of {xPuns::received::%uuid of arg-1%::*} < 1:
			message "&c&lxPuns&8 » &c%arg-1% &chas no punishment history"
			stop
		set {_rows} to xPunsGetRows(size of {xPuns::received::%uuid of arg-1%::*})
		open chest inventory with {_rows} rows named "History - %arg-1%" to player
		xPunsFormatRows(player, {_rows})
		if player has permission "xpuns.history.clear":
			set slot {_rows}*9-6 of player's current inventory to barrier named "&cClose"
			set slot {_rows}*9-4 of player's current inventory to paper named "&6Clear History"
		else:
			set slot {_rows}*9-5 of player's current inventory to barrier named "&cClose"
		set {_s} to -1
		loop {xPuns::received::%uuid of arg-1%::*}:
			add 1 to {_s}
			xPunsFormatHistory(player, {_s}, loop-value)
command /xpuns:reset [<text>]:
	permission: *
	permission message: &c&lxPuns&8 » {@noperm}
	trigger:
		if {xPuns::reset::%sender%} is not set:
			set {xPuns::reset::%sender%} to now
			message "&c&lxPuns&8 » &cWARNING: This command resets all variables. It should only be used in an emergency. If you wish to proceed, type the command again within 10 seconds."
			stop
		if difference between {xPuns::reset::%sender%} and now > 10 seconds:
			set {xPuns::reset::%sender%} to now
			message "&c&lxPuns&8 » &cWARNING: This command resets all variables. It should only be used in an emergency. If you wish to proceed, type the command again within 10 seconds."
			stop
		clear {xPuns::*}
		message "&c&lxPuns&8 » &aAll variables were reset"
		message "&c&lxPuns&8 » &cWARNING: You must manually reload the xPuns config file for the script to work properly!"
